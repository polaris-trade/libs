{
  "bootstrap-sha": "HEAD",
  "packages": {
    "config_loader": {
      "release-type": "rust",
      "package-name": "config_loader",
      "changelog-path": "config_loader/CHANGELOG.md"
    },
    "uuid_generator": {
      "release-type": "rust",
      "package-name": "uuid_generator",
      "changelog-path": "uuid_generator/CHANGELOG.md"
    }
  },
  "plugins": ["cargo-workspace"],
  "bump-minor-pre-major": true,
  "bump-patch-for-minor-pre-major": true,
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
